	"version": "0.2.0",
	"configurations": [
		{
			"name": "---------- TestServer",
		},
		{
			"name": "Run TestServer",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/node_modules/**"
			],
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/tests/TestServer/RunServer.js",
			"args": [],
			"outputCapture": "std",
			"internalConsoleOptions": "openOnSessionStart",
		},
		{
			"name": "---------- Unit Tests",
		},
		{
			"name": "-- All Unit Tests",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/node_modules/**"
			],
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/node_modules/mocha/bin/mocha",
			"args": [
				"-u",
				"bdd",
				"tests/UnitTests/*.tests.js",
				"--timeout",
				"0",
				"--slow",
				"50",
				"--colors",
			],
			"outputCapture": "std",
			"internalConsoleOptions": "openOnSessionStart",
		},
